{
    // General
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "git.ignoreMissingGitWarning": true,
    "update.channel": "none",
    "workbench.iconTheme": "vscode-icons",
    "editor.minimap.enabled": false,
    "workbench.editor.enablePreview": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.feedback.visible": false,
    "window.openFilesInNewWindow": "off",
    "window.openFoldersInNewWindow": "on",
    "editor.insertSpaces": true,
    "workbench.colorTheme": "Dark (Molokai)",
    "editor.fontFamily": "Hack, Consolas, 'Courier New', monospace",
    "editor.formatOnSave": true,
    "window.titleBarStyle": "custom",
    // Python
    "python.unitTest.unittestEnabled": true,
    "python.unitTest.unittestArgs": [
        "-s test",
        ".",
        "-p",
        "*test*.py"
    ],
    // Plugins
    "vsicons.dontShowNewVersionMessage": true,
    "scrolloff.alwaysCenter": false,
    "projectManager.projectsLocation": "C:\\Users\\natha\\Documents\\Computer Tools\\VSCodePortable\\Data\\code\\User",
    "git.enableSmartCommit": true,
    "latex-workshop.message.update.show": false,
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.latex.autoBuild.cleanAndRetry.enabled": true,
    "latex-workshop.latex.clean.enabled": true,
    "latex-workshop.latex.recipes": [
        {
            "name": "pdflatex",
            "tools": [
                "pdflatex",
            ]
        }
    ],
    // Vim
    "vim.easymotion": true,
    "vim.easymotionKeys": "hklyuiopnmertxcvbasdgjf",
    "vim.easymotionMarkerBackgroundColor": "#000",
    "vim.easymotionMarkerForegroundColorOneChar": "#fff",
    "vim.leader": ",",
    "vim.useSystemClipboard": true,
    "vim.cursorStylePerMode": {
        "normal": "block-outline",
        "insert": "line-thin",
        "replace": "underline"
    },
    "vim.statusBarColorControl": false,
    "vim.surround": true,
    "vim.foldfix": true,
    "vim.hlsearch": true,
    "vim.searchHighlightColor": "#75715E",
    // Vim Keybindings
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "leader",
                "c",
            ],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.commentLine"
                }
            ]
        },
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "n",
                "n"
            ],
            "after": [
                "<Esc>",
                "l"
            ]
        },
        {
            "before": [
                "leader",
                "m"
            ],
            "after": [
                "<Esc>",
                "l",
                "a"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "H"
            ],
            "after": [
                "0"
            ]
        },
        {
            "before": [
                "L"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "leader",
                "s"
            ],
            "after": [
                "leader",
                "leader",
                "s"
            ]
        },
        {
            "before": [
                "leader",
                "t",
                "t"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.files.save",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "leader",
                "h"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.quickOpen",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "leader",
                "w"
            ],
