        {
            "before": [
                "leader",
                "<CR>"
            ],
            "after": [],
            "commands": [
                {
                    "command": "jupyter.execCurrentCell"
                }
            ]
        }
    ],
    "window.zoomLevel": 0,
    "git.autofetch": true,
    "workbench.colorCustomizations": {
