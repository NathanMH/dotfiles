// Place your key bindings in this file to overwrite the defaults
[
    {"key": "ctrl+l", "command": "workbench.action.nextEditor"},
    {"key": "ctrl+h", "command": "workbench.action.previousEditor"},
    {"key": "ctrl+t", "command": "workbench.action.terminal.toggleTerminal"},
    {"key": "ctrl+c", "command": "workbench.debug.action.toggleRepl"},
    {"key": "ctrl+d", "command": "workbench.view.debug"},
    {"key": "ctrl+e", "command": "workbench.view.explorer"},
    {"key": "ctrl+g", "command": "workbench.view.git"},
    {"key": "ctrl+o", "command": "workbench.action.output.toggleOutput"},
    {"key": "ctrl+r", "command": "workbench.action.debug.start"},
    {"key": "ctrl+s", "command": "workbench.action.debug.stop"}
]
